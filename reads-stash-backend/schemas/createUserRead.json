{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "userId",
        "readId"
    ],
    "properties": {
        "rating": {
            "type": "number",
            "default": 0.0,
            "title": "The rating Schema",
            "examples": [
                4.5
            ]
        },
        "review_text": {
            "type": "string",
            "default": "",
            "title": "The review_text Schema",
            "examples": [
                "test review"
            ]
        },
        "review_date": {
            "type": "string",
            "default": "",
            "title": "The review_date Schema",
            "examples": [
                "2023-07-19"
            ]
        },
        "userId": {
            "type": "integer",
            "default": 0,
            "title": "The userId Schema",
            "examples": [
                1
            ]
        },
        "readId": {
            "type": "integer",
            "default": 0,
            "title": "The readId Schema",
            "examples": [
                2
            ]
        }
    },
    "examples": [{
        "rating": 4.5,
        "review_text": "test review",
        "review_date": "2023-07-19",
        "userId": 1,
        "readId": 2
    }]
}