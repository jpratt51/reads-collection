{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "username",
        "isbn"
    ],
    "properties": {
        "rating": {
            "type": "number",
            "default": 0.0,
            "title": "The rating Schema",
            "examples": [
                4.5
            ]
        },
        "review_text": {
            "type": "string",
            "default": "",
            "title": "The review_text Schema",
            "examples": [
                "test review"
            ]
        },
        "review_date": {
            "type": "string",
            "default": "",
            "title": "The review_date Schema",
            "examples": [
                "2023-07-19"
            ]
        },
        "username": {
            "type": "string",
            "default": "",
            "title": "The username Schema",
            "examples": [
                "testUsername"
            ]
        },
        "isbn": {
            "type": "string",
            "default": "",
            "title": "The isbn Schema",
            "pattern" : "^[0-9]+$",
            "examples": [
                "9780989671767"
            ]
        }
    },
    "examples": [{
        "rating": 4.5,
        "review_text": "test review",
        "review_date": "2023-07-19",
        "username": "testUsername",
        "isbn": "9780989671767"
    }]
}