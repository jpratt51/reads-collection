{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "name",
        "user_username"
    ],
    "properties": {
        "name": {
            "type": "string",
            "maxLength" : 30,
            "minLength" : 5,
            "default": "",
            "title": "The name Schema",
            "examples": [
                "testBadge"
            ]
        },
        "user_username": {
            "type": "string",
            "maxLength" : 30,
            "minLength" : 5,
            "default": "",
            "title": "The username Schema",
            "examples": [
                "testUsername"
            ]
        }
    },
    "examples": [{
        "name": "testBadge",
        "user_username": "testUsername"
    }]
}