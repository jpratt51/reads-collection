{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "isbn",
        "collectionId"
    ],
    "properties": {
        "isbn": {
            "type": "string",
            "default": "",
            "title": "The isbn Schema",
            "pattern" : "^[0-9]+$",
            "examples": [
                "9780989671767"
            ]
        },
        "collectionId": {
            "type": "integer",
            "default": 0,
            "title": "The collectionId Schema",
            "examples": [
                2
            ]
        }
    },
    "examples": [{
        "isbn": "9780989671767",
        "collectionId": 2
    }]
}