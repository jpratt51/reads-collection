{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "title",
        "text",
        "userId"
    ],
    "properties": {
        "title": {
            "type": "string",
            "default": "",
            "maxLength": 50,
            "title": "The title Schema",
            "examples": [
                "sample journal title"
            ]
        },
        "text": {
            "type": "string",
            "default": "",
            "maxLength": 7500,
            "title": "The text Schema",
            "examples": [
                "sample journal entry"
            ]
        },
        "userId": {
            "type": "integer",
            "default": 0,
            "title": "The userId Schema",
            "examples": [
                1
            ]
        }
    },
    "examples": [{
        "title": "sample journal title",
        "text": "sample journal entry",
        "userId": 1
    }]
}